@{
    ViewData["Title"] = "Home Page";
}
<!--로그인/회원가입-->
<div class="position-absolute text-center w-300px t10_r10 min-h-200 border" style="">
    <div class="w-180px m-auto mt_3">
        <input type="text" placeholder="ID" />
        <input type="password" placeholder="PW" />
        <div class="mt-3">
            <button type="submit" class="btn_login mr-3">로그인</button>
            <button type="submit" class="btn_join">회원가입</button>
        </div>
    </div>
</div>

<!--검색-->
<div class="m-5">
    <div class="w-50 m-auto text-center">
        <div class="mt-10">
            <div class="w-50 m-auto text-right">
                <div class="d-inline">
                    <input type="text" class="w-85 border-0 outline0" autofocus />
                </div>
                <a href="#">
                    <img src="~/img/search.png" alt="search" />
                </a>
            </div>
            <p class="w-50 border-bottom border-dark m-auto">
            </p>
        </div>
    </div>
</div>

<!--자주하는 질문-->
<div class="w-100 border" style="margin-top: 17%; height:1000px;">
    <div>자주하는질문</div>
</div>